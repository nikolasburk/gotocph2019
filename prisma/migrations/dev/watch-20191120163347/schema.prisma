datasource db {
  provider = "postgresql"
  url = "***"
}

generator photonjs {
  provider = "photonjs"
}

model User {
  id    String  @id @default(cuid())
  name  String?
  email String  @unique
  posts Post[]
}

model Post {
  id        String  @id @default(cuid())
  title     String
  content   String?
  published Boolean @default(false)
  author    User
}